<Project>

  <ItemGroup>
      <!-- Exclude mlaunch binaries from signing - they are already signed by Mono -->
    <ItemsToSign Remove="$(IntermediateOutputPath)**\MonoBundle\*.exe" />
    <ItemsToSign Remove="$(IntermediateOutputPath)**\MonoBundle\*.dll" />
    <ItemsToSign Remove="$(ArtifactsTmpDir)**\MonoBundle\*.exe" />
    <ItemsToSign Remove="$(ArtifactsTmpDir)**\MonoBundle\*.dll" />

    <ItemsToSign Remove="System.Net.Http.dll" />
    <ItemsToSign Remove="System.Net.Http.dll" />
    <ItemsToSign Remove="System.Numerics.dll" />
    <ItemsToSign Remove="System.Numerics.dll" />
    <ItemsToSign Remove="System.Xml.dll" />
    <ItemsToSign Remove="System.Xml.dll" />
    <ItemsToSign Remove="Xamarin.Hosting.dll" />
    <ItemsToSign Remove="Xamarin.Hosting.dll" />
    <ItemsToSign Remove="Xamarin.Mac.dll" />
    <ItemsToSign Remove="Xamarin.Mac.dll" />

  </ItemGroup>

  <PropertyGroup>
    <!--
      Signing of shipping artifacts (layout, msi, bundle) are handled separately.
      It is therefore expected that above removal can yield an empty set.
    -->
    <AllowEmptySignList>true</AllowEmptySignList>
  </PropertyGroup>

</Project>
