<Project>

  <ItemGroup>
    <FileSignInfo Include="**/MonoBundle/mlaunch.exe" CertificateName="3PartySHA2" />
    <FileSignInfo Include="**/MonoBundle/System.Net.Http.dll" CertificateName="3PartySHA2" />
    <FileSignInfo Include="**/MonoBundle/System.Numerics.dll" CertificateName="3PartySHA2" />
    <FileSignInfo Include="**/MonoBundle/System.Xml.dll" CertificateName="3PartySHA2" />
    <FileSignInfo Include="**/MonoBundle/Xamarin.Hosting.dll" CertificateName="3PartySHA2" />
    <FileSignInfo Include="**/MonoBundle/Xamarin.Mac.dll" CertificateName="3PartySHA2" />
    <FileSignInfo Include="**/MonoBundle/Mono.Security.dll" CertificateName="3PartySHA2" />
    <FileSignInfo Include="**/MonoBundle/mscorlib.dll" CertificateName="3PartySHA2" />
    <FileSignInfo Include="**/MonoBundle/System.Core.dll" CertificateName="3PartySHA2" />
    <FileSignInfo Include="**/MonoBundle/System.dll" CertificateName="3PartySHA2" />
  </ItemGroup>

  <PropertyGroup>
    <!--
      Signing of shipping artifacts (layout, msi, bundle) are handled separately.
      It is therefore expected that above removal can yield an empty set.
    -->
    <AllowEmptySignList>true</AllowEmptySignList>
  </PropertyGroup>

</Project>
